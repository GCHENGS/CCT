<Application x:Class="CCT.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:CCT">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resource\Dictionarys\ThemeResource\ThemeResources.xaml" />
                <ResourceDictionary Source="Resource\Dictionarys\StyleResource\StyleResources.xaml" />
                <ResourceDictionary Source="Resource\Dictionarys\StringResource\StringResources.xaml" />
                <ResourceDictionary Source="Resource\Dictionarys\ImageResource\ImageResources.xaml" />
                <ResourceDictionary Source="Resource\Dictionarys\ConverterResource\ConverterResources.xaml" />
                <ResourceDictionary Source="Resource\Dictionarys\DataTemplateResource\DataTemplateResources.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
