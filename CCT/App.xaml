<Application x:Class="CCT.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:CCT">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Fluent;Component/Themes/Generic.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Resource/Dictionarys/ImageResource/ImageResources.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Resource/Dictionarys/ConverterResource/ConverterResources.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Resource/Dictionarys/DataTemplateResource/DataTemplateResources.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Resource/Dictionarys/ThemeResource/ThemeResources.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Resource/Dictionarys/StringResource/En_US.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Resource/Dictionarys/StringResource/Zh_CN.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Resource/Dictionarys/StyleResource/StyleResources.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
